    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3.3"
    },
    "suggest": {
        "presta/presta-cms": "1.0.*"
    },
    "autoload": {
        "psr-0": { "Presta\\PimpMySonataBundle": "" }
    },
    "target-dir": "Presta/PimpMySonataBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
