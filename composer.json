{
    "name": "presta/pimp-my-sonata-bundle",
    "type": "symfony-bundle",
    "description": "PrestaPimpMySonataBundle : Same concept as Pimp My Ride TV show but for SonataAdminBundle",
    "keywords": ["symfony2", "bundle", "sonata-project", "design", "prestaconcept"],
    "homepage": "https://github.com/prestaconcept/PrestaConfigurationBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Nicolas Bastien",
            "email": "nbastien@prestaconcept.net",
            "homepage": "http://www.prestaconcept.net/"
        }
